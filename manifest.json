{
	"name": "VG News",
	"description": "Makes the front page of VG.no look like Hacker News.",
	"manifest_version": 2,
	"version": "0.4.0",

	"icons": {
		"128": "128.png"
	},
	
	"content_scripts": [
		{
			"matches": ["https://*.vg.no/*"],
			"js": ["jquery-3.1.1.min.js", "vgnews.js"]
		}
	],
	"web_accessible_resources": [
		"128.png"
	]
}